import random
halloween_monsters = ["szkielet", "strider", "pumpkin", "smurf cat", "mumia", "vampire",]
monster = random.choice(halloween_monsters)
if monster == "szkielet":
  print("szkielet to kosci")
elif monster == "zoombie":
  print("zoombie to pan od polskiego i pani od histori")
elif monster == "vampire":
  print("vampire to pani od chemi która każe wykuwać tablice mendelejewa")
elif monster == "pumpkin":
  print("pumpkin to pan od wf który nazywa się adam musiał i musi zjeść jacka placka")
elif monster == "smurf cat":
  print("smurf cat to rosyjski kot którego nazwałem putin")
elif monster == "mumia":
  print("mumia to pani od geografi która mówi że smerfy są z polski i są zielone")
elif monster == "strider":
  print("strider to pan od angielskiego który jest wredny")
  
